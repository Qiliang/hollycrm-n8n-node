{
  "name": "n8n-nodes-dashscope",
  "version": "0.1.0",
  "description": "n8n node for Aliyun DashScope (Qwen) API",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "dashscope",
    "qwen",
    "aliyun",
    "ai"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Qiliang/hollycrm-n8n-node",
  "author": {
    "name": "Qiliang"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Qiliang/hollycrm-n8n-node"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DashScopeApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/DashScope/DashScope.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
